{
  "kind": "ExecutorDeployment",
  "metadata": {
    "name": "docker-executor-linux-server"
  },
  "handler": {
    "executorType": "docker-reconciler-home-linux-server",
    "executorName": "docker-reconciler-home-linux-server"
  },
  "spec": {
    "image": "colonyspaces/dockerexecutor:latest",
    "replicas": 1,
    "executorType": "container-executor",
    "volumes": [
      {
        "host": "/var/run/docker.sock",
        "container": "/var/run/docker.sock"
      },
      {
        "host": "/tmp/colonies",
        "container": "/tmp/colonies"
      }
    ],
    "privileged": true,
    "env": {
      "EXECUTOR_TYPE": "container-executor",
      "EXECUTOR_HW_MODEL": "Z790 Pro RS",
      "EXECUTOR_HW_CPU": "13th Gen Intel Core i9-13900K",
      "EXECUTOR_HW_CPU_CORES": "24",
      "EXECUTOR_HW_MEM": "64056 MB",
      "EXECUTOR_HW_STORAGE": "1000 GB",
      "EXECUTOR_HW_PLATFORM": "linux",
      "EXECUTOR_HW_ARCHITECTURE": "amd64",
      "EXECUTOR_HW_GPU_NAME": "NVIDIA GeForce RTX 3080 Ti",
      "EXECUTOR_HW_GPU_COUNT": "1",
      "EXECUTOR_HW_GPU_MEM": "12288 MB",
      "EXECUTOR_HW_NODES": "1",
      "EXECUTOR_HW_GPU_NODES_COUNT": "1",
      "EXECUTOR_LOCATION_LONG": "0",
      "EXECUTOR_LOCATION_LAT": "0",
      "EXECUTOR_LOCATION_NAME": "Home",
      "EXECUTOR_LOCATION_DESC": "Intel i9 + RTX 3080 Ti Server"
    }
  }
}
